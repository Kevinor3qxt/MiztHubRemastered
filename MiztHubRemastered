-- Example made by twink marie
local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local Window = Material.Load({
    Title = "Mizt Hub Remastered",
    Style = 3,
    SizeX = 500,
    SizeY = 350,
    Theme = "Light",
    ColorOverrides = {
        MainFrame = Color3.fromRGB(235,235,235)
    }
})

local ReTab = Window.New({
    Title = "Reanimations"
})

local FeTab = Window.New({
    Title = "Fe Scripts"
})

local Button = ReTab.Button({
    Text = "Simple Reanimate",
    Callback = function()
        		_G.MiztReanimSettings = {
    PermanentDeath = false,
    NoHeadPermanentDeath = false,
    Noclip = true,
    HatReanimate = false,
    SemiHatReanimate = false,
    UseMizaruTorso = false,
    FlingActive = false,
    AlignMethod = true,
    Netless = true,
    ActiveLegacyNet = true,
    NetlessVelocity = {0,0,-50}
}

loadstring(game:HttpGet('https://raw.githubusercontent.com/Sylixe/Scripts/main/MiztReanimate.lua',true))()
    end,
})
local Button = ReTab.Button({
    Text = "PermaDeath Reanimate",
